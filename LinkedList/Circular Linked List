public class Main
{
    public static void main(String[] args)
    {
        CLL list = new CLL();
        list.insertFirst(9);
        list.insertFirst(8);
        list.insertLast(2);
        list.delete(9);
        list.display();
    }
}

class CLL
{
    private Node head;
    private Node tail;
    
    public void insertFirst(int val)
    {
        Node node = new Node(val);
        if(head == null)
        {
            head = node;
            tail = head;
            tail.next = head;
            return;
        }
        node.next = head;
        head = node;
        tail.next = head;
    }
    
    public void insertLast(int val)
    {
        Node node = new Node(val);
        if(head == null)
        {
            head = node;
            tail = head;
            tail.next = head;
            return;
        }
        tail.next = node;
        tail = node;
        tail.next = head;
    }
    
    public void delete(int val)
    {
        if(head.value == val)
        {
            tail.next = head.next;
            head = head.next;
            return;
        }
        Node temp1 = head;
        Node temp2 = head.next;
        while(temp2 != head)
        {
            if(temp2.value == val)
            {
                temp1.next = temp2.next;
                return;
            }
            temp1 = temp1.next;
            temp2 = temp2.next;
        }
    }
    
    public void display()
    {
        System.out.print(head.value + " " + "->" + " ");
        Node temp = head.next;
        while(temp != head)
        {
        System.out.print(temp.value + " " + "->" + " ");
        temp = temp.next;
        }
    }
    
    
    
    
    class Node
    {
        private int value;
        private Node next;
        
        Node (int val)
        {
            this.value = val;
        }
    }
}
