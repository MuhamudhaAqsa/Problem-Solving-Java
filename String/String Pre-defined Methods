import java.util.Scanner;
import java.util.Arrays;
public class Main
{
    public static void main(String[] args)
    {
        Scanner input = new Scanner(System.in);
        
        String s = input.nextLine();
        
        int l = s.length();
        
        System.out.println(l);
        
        char character = s.charAt(2);
        
        System.out.println(character);
        
        String subString = s.substring(10, s.length());
        System.out.println(subString);
        
        int index = s.indexOf('A');
        System.out.println(index);
        
        int position = s.indexOf(" Aqsa");
        System.out.println(position);
        
        if(s.equals("Muhamudha Aqsa"))
        {
            System.out.println(true);
        }
        
        String string = "MUHAMUDHA AQSA";
        
        if(s.equals(string))
        {
            System.out.println(true);
        }
        else
        {
            System.out.println(false);
        }
        
        if(s.equalsIgnoreCase(string))
        {
            System.out.println(true);
        }
        
        string = string.toLowerCase();
        System.out.println(string);
        
        string = string.toUpperCase();
        System.out.println(string);
        
        String sentence = " Good Morning ";
        sentence = sentence.trim();
        System.out.println(sentence);
        System.out.println(sentence.length());
        
        boolean b = sentence.contains("Morning");
        System.out.println(b);
        
        String[] arr = sentence.split("o");
        System.out.println(Arrays.toString(arr));
        
        String s1 = "Muhamudha";
        String s2 = " ";
        String s3 = "Aqsa";
        String concatenation = s1.concat(s2);
        concatenation = concatenation.concat(s3);
        System.out.println(concatenation);

       char[] characterArray = concatenation.toCharArray();
       System.out.println(Arrays.toString(characterArray));
    }
}
