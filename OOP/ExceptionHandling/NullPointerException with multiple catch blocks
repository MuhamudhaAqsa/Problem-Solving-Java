public class Main
{
	public static void main(String[] args) 
	{
		try
		{
		    String string = null;
		    System.out.println(string.length());
		}
		catch(ArithmeticException e)
		{
		    System.out.println(e.getMessage());
		}
		catch(NullPointerException e)
		{
		    System.out.println(e.getMessage());
		}
	}
}
