class Box
{
    double l;
    double w;
    double h;
    double weight;
    
    Box ()
    {
        super();
        
        this.l = -1;
        this.w = -1;
        this.h = -1;
    }
    
    Box (double side)
    {
        this.w = side;
        this.l = side;
        this.h = side;
    }
    
    Box (double l, double h, double w)
    {
        this.l = l;
        this.h = h;
        this.w = w;
    }
    
    Box (Box old)
    {
        this.h = old.h;
        this.l = old.l;
        this.w = old.w;
    }
    
    public void information()
    {
        System.out.println("Box is lavender");
    }
    
}

class BoxWeight extends Box
{
    double weight;
    
    BoxWeight()
    {
        this.weight = -1;
        this.l = 18;
    }
    
    BoxWeight (BoxWeight other)
    {
        super(other);
        this.weight = other.weight;
    }
    
    BoxWeight (double l, double h, double w, double weight)
    {
        super(l, h, w);
        
        this.weight = weight;
    }
    
}

class BoxPrice extends BoxWeight
{
    double price;
    
    BoxPrice ()
    {
        super();
        this.price = 20.0;
    }
    
    BoxPrice (double l, double h, double w, double weight, double price)
    {
        super(l, h, w, weight);
        this.price = price;
    }
    
    
}

